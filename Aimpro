local SilentAim = loadstring(game:HttpGet(
    "https://raw.githubusercontent.com/thanhgaylolgbt-maker/Aim/refs/heads/thanhgaylolgbt-maker-patch-1/Aim.lua.txt"
))()

SilentAim:SetPlayerSilentAim(true)
SilentAim:SetNPCSilentAim(true)

SilentAim:SetZSkillorM1(true)

SilentAim:SetDistanceLimit(800)
SilentAim:SetLowHPPriority(true)

SilentAim:SetAimSmoothness(0.3)
SilentAim:SetVelocitySampleCount(4)
SilentAim:EnableProjectileLead(false)
